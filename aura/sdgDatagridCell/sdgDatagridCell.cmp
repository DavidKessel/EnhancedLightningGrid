<aura:component >
    <aura:handler name="init" value="{!this}" action="{!c.onInit}" />
    <aura:attribute name="renderfield" type="object" access="global" />
    <!-- Attributes added for navigating on Community pages. -->
    <aura:attribute name="IsCommunity" type="Boolean" access="global" />    
    <aura:attribute name="URL" type="Object" access="global" />
    <!--  -->
    <aura:attribute name="SDG" type="object" access="global" />
    <aura:attribute name="sdgAgo" type="String" access="public" default="{!$Label.c.sdgAgo}" />
    <aura:attribute name="sdgIn" type="String" access="public" default="{!$Label.c.sdgIn}" />
  
    <td data-label="{#v.renderfield.FieldLabel}" class="pullleft pullup">

    <div class="slds-truncate" onclick="{!c.HandleURLClick}">{!v.body}</div>
       
    </td>
</aura:component>